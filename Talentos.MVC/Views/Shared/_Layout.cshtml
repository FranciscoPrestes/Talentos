<body>

<div id="divBlock"></div>

<div class="image-container">


    <div class="logo-container">
        <div class="logo">
            <img src="~/Content/img/logoMeusPedidosAzul.png">
        </div>

    </div>


    <!-- Big container -->
    <div class="container js-Conteudo">


        @RenderBody()


    </div> <!-- big container -->



    <div class="container js-Mensagem">





    </div> <!-- big container -->



    <div class="footer">
        <div class="container">
            &copy; Francisco Prestes - @DateTime.Now.Year
        </div>
    </div>
</div>





<div id="loader"></div>
<div style="display: none;" id="divLoad" class="animate-bottom">
    @*<h2>Pronto!</h2>
        <p>Agora é só aguardar nosso email.</p>*@
</div>

@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrap")
@RenderSection("scripts", required: false)
</body>
<!--   Core JS Files   -->
<script src="~/Content/bootstrapWizard/assets/js/jquery-2.2.4.min.js" type="text/javascript"></script>
